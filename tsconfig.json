{
	"compilerOptions": {
		"target": "ES2022",
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"lib": [
			"ES2022",
			"dom"
		]
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"spec",
		"scripts"
	]
}
