{
	"name": "ejv",
	"version": "2.1.3",
	"description": "Easy JSON Validator",
	"exports": {
		"require": "./build/cjs/index.js",
		"import": "./build/esm/index.js",
		"types": "./build/index.d.ts"
	},
	"types": "build/index.d.ts",
	"main": "build/cjs/index.js",
	"scripts": {
		"clean": "rimraf ./build",
		"prebuild": "yarn clean",
		"build:types": "tsc -p tsconfig.types.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build": "yarn build:types&&yarn build:cjs&&yarn build:esm",
		"postbuild": "tsc -p tsconfig.scripts.json&&node build/scripts/add-js-extensions&&node build/scripts/create-package-json",
		"test": "mocha",
		"lint": "eslint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/han41858/ejv.git"
	},
	"keywords": [
		"json",
		"validator",
		"validation"
	],
	"author": "Janghyun Han <han41858@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/han41858/ejv/issues"
	},
	"homepage": "https://github.com/han41858/ejv#readme",
	"devDependencies": {
		"@eslint/js": "^9.30.1",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.16.0",
		"@typescript-eslint/eslint-plugin": "^8.35.1",
		"@typescript-eslint/parser": "^8.35.1",
		"chai": "^5.2.0",
		"eslint": "^9.30.1",
		"eslint-plugin-chai-friendly": "^1.1.0",
		"globals": "^16.3.0",
		"mocha": "^11.7.1",
		"rimraf": "^6.0.1",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1"
	},
	"packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
